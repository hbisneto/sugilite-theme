		"activityBarBadge.foreground": "#FFF",
		"activityBarBadge.background": "#ca64ea",
		"tab.inactiveBackground": "#000",
		"tab.activeBackground": "#1F1F1F",
		"tab.lastPinnedBorder": "#7624a7",
		"sideBar.background": "#141414",
		"statusBar.background": "#7624a7",
		"statusBar.noFolderBackground": "#7624a7",
		"statusBarItem.remoteBackground": "#ca64ea",
		"ports.iconRunningProcessForeground": "#7624a7",
		"editorGroupHeader.tabsBackground": "#000",
		"titleBar.activeBackground": "#7624a7",
		"titleBar.inactiveBackground": "#7624a7",
		"selection.background": "#ADD6FF20",
		"editor.background": "#1F1F1F",
		"editorGroup.border": "#000",
		"editorCursor.foreground": "#FFFFFF",
		"editor.foreground": "#FFFFFF",
		"editorWhitespace.foreground": "#C4C4C4",
		"editor.selectionBackground": "#ADD6FF20",
		"minimap.selectionHighlight": "#7624a7",
		"editorLineNumber.foreground": "#C4C4C4",
		"editorLineNumber.activeForeground": "#7624a7",
		"editorWidget.background": "#141414",
		"editorHoverWidget.background": "#454545",
		"editorSuggestWidget.background": "#000",
