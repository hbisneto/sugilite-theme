{
	"$schema": "vscode://schemas/color-theme",
	"name": "Sugilite Tokens",
	"colors": {
		"activityBar.background": "#000",
		"activityBar.foreground": "#ca64ea",
		"activityBar.inactiveForeground": "#9d9d9d",
		"activityBar.activeBackground": "#000",
